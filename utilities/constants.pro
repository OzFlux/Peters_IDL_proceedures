PRO Constants

 COMMON Constants, g, k, cp, lv, Rd, Rv

; *** General boundary-layer constants.
  g = 9.8			; Gravition constant, m/s2
  k = 0.4			; von Karmans constant
  cp = 1004.67		; Specific heat at constant pressure, J/K/kg
  lv = 2453600.		; Latent heat of vapouristaion, J/K/kg
  Rd = 287.			; Dry air gas constant, J/K/kg
  Rv = 461.			; Water vapour gas constant, J/K/kg

END